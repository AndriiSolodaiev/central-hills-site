
extends ../layout/main
block variable
  -
    var title_page = 'contacts'
    var id_page = 'id-page-contacts'
    var class_page = 'class-page-contacts'
    var headerCustomClass = 'type1' 
    var breadcrumbsSubitem = [
      {
        label: 'About',
        link: pagesLink[0].about
      }
    ]

block meta
  title= title_page
  meta(name='description' content='')
block head
  link(rel='stylesheet' href="./assets/styles/main.min.css")
block scripts
  script(defer src='./assets/scripts/vendors.bundle.js')
  script(defer src='./assets/scripts/header.bundle.js')
  script(defer src='./assets/scripts/footer.bundle.js')
  script(defer src='./assets/scripts/contacts.bundle.js')
block content
  section.documents.container--mobile
    h1.page__title Контакти
    .contacts__map.map(id="map")
      //- <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d42414.982348961654!2d24.352758635553503!3d48.36174580883851!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x473719c19227218b%3A0x86600928eb33c293!2sMountain%20Residence!5e0!3m2!1suk!2sua!4v1695914623172!5m2!1suk!2sua" width="100%" height="100%" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>

block footer
  include ../includes/footer.pug

block header
  include ../includes/header.pug
